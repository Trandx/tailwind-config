{
  "name": "@package/tailwind-config",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/lib/index.js",
  "module": "dist/lib/index.js",
  "types": "dist/types/index.d.ts",
  "keywords": [
    "tailwind",
    "config"
  ],
  "scripts": {
    "build": "tsc && npm run cp && npm run mv-pkg-json",
    "cp": "find ./ -maxdepth 1 -type f \\( -name 'pub.*.json' -o -name 'README.md' \\) -exec cp {} ./dist \\; ",
    "mv-pkg-json": "cd ./dist && mv pub.package.json package.json && node ../scripts/update-dist-package-json.js"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@tailwindcss/forms": "^0.5.9",
    "@types/node": "^20.16.15",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  }
}
